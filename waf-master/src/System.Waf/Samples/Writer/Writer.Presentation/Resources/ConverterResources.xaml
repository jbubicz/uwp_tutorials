<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:waf="http://waf.codeplex.com/schemas"
                    xmlns:c="clr-namespace:Waf.Writer.Presentation.Converters">

    <waf:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>

    <waf:InvertBooleanConverter x:Key="InvertBooleanConverter"/>

    <c:DoubleToZoomConverter x:Key="DoubleToZoomConverter"/>

    <c:MenuFileNameConverter x:Key="MenuFileNameConverter"/>

    <c:PercentConverter x:Key="PercentConverter"/>

    <c:TabFileNameConverter x:Key="TabFileNameConverter"/>

    <c:TitleConverter x:Key="TitleConverter"/>
    
</ResourceDictionary>